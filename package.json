{
  "name": "stumpfi-io",
  "version": "1.0.0",
  "description": "Converts stumpfi documents to HTML and vice-versa.",
  "repository": "https://github.com/matthieujabbour/stumpfi-io",
  "homepage": "https://github.com/matthieujabbour/stumpfi-io",
  "main": "./src/main.ts",
  "keywords": [
    "stumpfi",
    "documents",
    "html",
    "convert"
  ],
  "author": "Matthieu JABBOUR <matthieu.jabbour@gmail.com> (https://github.com/matthieujabbour)",
  "contributors": [
    {
      "name": "Matthieu JABBOUR",
      "url": "https://github.com/matthieujabbour"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthieujabbour/stumpfi-io/issues"
  },
  "types": "./config/types.d.ts",
  "devDependencies": {
    "@types/fs-extra": "^4.0.0",
    "@types/jest": "^21.1.0",
    "@types/webpack": "^3.0.9",
    "awesome-typescript-loader": "^3.2.3",
    "jest": "^21.1.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-stumpfi": "^1.0.7",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.9.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "fs-extra": "^4.0.1",
    "stumpfi": "^3.0.0"
  },
  "scripts": {
    "start": "ts-node scripts/start",
    "build": "ts-node scripts/build",
    "test": "ts-node scripts/test --coverage --watch",
    "doc": "./node_modules/.bin/typedoc --out ./doc/ src/"
  },
  "engines": {
    "node": ">= 4"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
